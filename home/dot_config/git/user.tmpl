[user]
{{- $github := (index . "github" | default (dict "id" "" "name" "" "login" "")) -}}
{{- with $github -}}
{{- if and .id .name .login }}
    name = {{ .name | quote }}
    email = {{ printf "%v+%s@users.noreply.github.com" .id .login | quote }}
{{- end }}
{{- end }}
    signingkey = ~/.ssh/id_ed25519.pub
